 313  apt install default-jre
  314  wget -q -O - https://pkg.jenkins.io/debian-stable/jenkins.io.key | sudo apt-key add -
  315  sudo sh -c 'echo deb https://pkg.jenkins.io/debian-stable binary/ > \
    /etc/apt/sources.list.d/jenkins.list'
  316  sudo apt-get update
  317  sudo apt-get install jenkins
  
  319  systemctl status jenkins
